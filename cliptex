#!/bin/bash
# If the command is called with no arguments, then use clipboard
if [ -z "$*" ]; then
    clip="$(wl-paste)"
# Otherwise forward the arguments to pnglatex
else
    clip="$*"
fi
pnglatex -d 300 -f "$clip" -o .temp.png -e align* -p amssymb:amsmath:cancel -S
wl-copy < .temp.png
rm .temp.png